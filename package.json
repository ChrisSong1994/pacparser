{
  "name": "pacparser",
  "version": "0.0.6",
  "keywords": [
    "pac",
    "pacparser",
    "proxy",
    "pac-proxy-agent"
  ],
  "description": "A library to parse proxy auto-config (PAC) files in JavaScript.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "pacparser": "./lib/bin/index.js",
    "pap": "./lib/bin/index.js"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "npm run clean && concurrently --kill-others \"tsc --sourceMap -w\" \"tsc-alias -w\"",
    "build": "npm run clean && tsc  && tsc-alias",
    "cli": "node ./lib/bin/index.js",
    "clean": "rimraf lib",
    "prettier": "prettier --write 'src/**/*.ts'",
    "pub": "npm publish --access=public",
    "pub:beta": "npm publish --access=public --tag=beta",
    "website": "serve ./website",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "type": "module",
  "author": "ChrisSong1994 <https://github.com/ChrisSong1994>",
  "repository": {
    "type": "git",
    "url": "git@github.com:ChrisSong1994/pacparser.git"
  },
  "homepage": "https://chrissong1994.github.io/pacparser",
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@vitest/coverage-v8": "3.2.4",
    "concurrently": "^9.2.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "serve": "^14.2.5",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "dns-lookup-sync": "^1.0.0"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.12.3"
}
